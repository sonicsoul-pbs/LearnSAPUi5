<mvc:View
    controllerName="learnsapui5.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Page title="{ i18n>homePageTitle }">
        <headerContent>
            <Button 
                icon="sap-icon://hello-world" 
                press="onOpenDialog"/>
        </headerContent>
        <content>
            <mvc:XMLView viewName="learnsapui5.view.HelloPanel"></mvc:XMLView>
            <mvc:XMLView viewName="learnsapui5.view.InvoiceList"></mvc:XMLView>
        </content>
    </Page>
</mvc:View>